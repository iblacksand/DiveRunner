<Window x:Class="DiveRunner.InputBox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DiveRunner"
        mc:Ignorable="d"
        Title="InputBox" Height="291.832" Width="494.802" ResizeMode="CanMinimize" Closing="Window_Closing">
    <Grid>
        <TextBlock x:Name="TitleBlock" HorizontalAlignment="Left" Margin="10,22,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="28" Width="470" RenderTransformOrigin="0.502,0.536" FontSize="18" TextAlignment="Center"/>
        <TextBox x:Name="InputTextBox" HorizontalAlignment="Left" Height="24" Margin="134,122,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="208"/>
        <Button x:Name="OkButton" Content="Ok" HorizontalAlignment="Left" VerticalAlignment="Top" Width="74" Margin="210,183,0,0" Click="OkButton_Click"/>

    </Grid>
</Window>
