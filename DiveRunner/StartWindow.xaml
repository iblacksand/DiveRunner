<Window x:Class="DiveRunner.StartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DiveRunner"
        mc:Ignorable="d"
        Title="Start Menu" Height="290.247" Width="425.923">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Button x:Name="LoadButton" Margin="30,30,30,30" Grid.Row="1" Grid.Column="1" Content="Load Meet" Click="LoadButton_Click" />
        <Button x:Name="NewMeetButton" Margin="30,30,30,30" Grid.Row="2" Grid.Column="1" Content="New Meet" Click="NewMeetButton_Click"/>
        <TextBlock Margin="0,30,0,0" Grid.Row="0" Grid.Column="0" HorizontalAlignment="Center"  TextWrapping="Wrap" Text="Dive Meet Runner" FontWeight="Bold" FontSize="36" RenderTransformOrigin="-0.015,0.567" Grid.ColumnSpan="3"/>
    </Grid>


</Window>
